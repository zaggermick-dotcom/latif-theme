{%- # JAVASCRIPT - TIMER DYNAMIQUE + SCROLL -%}
{%- # â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -%}

<script>
(function() {
  var sectionId = '{{ section.id }}';
  
  /* â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
     TIMER COUNTDOWN
     â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• */
  {%- if section.settings.show_timer -%}
  (function initTimer() {
    var hoursEl = document.getElementById('HeroVipHours-' + sectionId);
    var minsEl = document.getElementById('HeroVipMins-' + sectionId);
    var secsEl = document.getElementById('HeroVipSecs-' + sectionId);
    
    if (!hoursEl || !minsEl || !secsEl) return;
    
    var timerHours = {{ section.settings.timer_hours | default: 24 }};
    var timerMinutes = {{ section.settings.timer_minutes | default: 0 }};
    var timerSeconds = {{ section.settings.timer_seconds | default: 0 }};
    var totalDuration = (timerHours * 3600) + (timerMinutes * 60) + timerSeconds;
    
    var storageKey = 'heroVipTimer_' + sectionId + '_v2';
    var savedEndTime = localStorage.getItem(storageKey);
    var endTime;
    
    if (savedEndTime) {
      var parsed = parseInt(savedEndTime, 10);
      if (parsed > Date.now()) {
        endTime = parsed;
      } else {
        endTime = Date.now() + (totalDuration * 1000);
        localStorage.setItem(storageKey, endTime.toString());
      }
    } else {
      endTime = Date.now() + (totalDuration * 1000);
      localStorage.setItem(storageKey, endTime.toString());
    }
    
    function tick() {
      var now = Date.now();
      var remaining = Math.max(0, Math.floor((endTime - now) / 1000));
      
      if (remaining <= 0) {
        endTime = Date.now() + (totalDuration * 1000);
        localStorage.setItem(storageKey, endTime.toString());
        remaining = totalDuration;
      }
      
      var h = Math.floor(remaining / 3600);
      var m = Math.floor((remaining % 3600) / 60);
      var s = remaining % 60;
      
      hoursEl.textContent = String(h).padStart(2, '0');
      minsEl.textContent = String(m).padStart(2, '0');
      secsEl.textContent = String(s).padStart(2, '0');
    }
    
    tick();
    setInterval(tick, 1000);
  })();
  {%- endif -%}
  
  /* â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
     SMOOTH SCROLL VERS PACK SELECTOR - CORRIGÃ‰ POUR LATIF
     â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• */
  (function initScroll() {
    var ctaBtn = document.getElementById('HeroVipCta-' + sectionId);
    if (!ctaBtn) return;
    
    ctaBtn.addEventListener('click', function(e) {
      e.preventDefault();
      
      var target = null;
      
      // SÃ©lecteurs SPÃ‰CIFIQUES pour ton pack selector LATIF
      var selectors = [
        '.product-pack-conversion',
        '.pack-conversion__wrapper',
        '.pack-conversion__card',
        '.pack-multi-wrapper',
        'product-pack',
        '[data-pack-index]',
        '#product-pack-form-',
        '.pack-countdown-ultra',
        'form[id^="product-pack-form"]'
      ];
      
      // Essayer chaque sÃ©lecteur
      for (var i = 0; i < selectors.length; i++) {
        var found = document.querySelector(selectors[i]);
        if (found) {
          target = found;
          console.log('Pack trouvÃ© avec:', selectors[i]);
          break;
        }
      }
      
      // Si trouvÃ©, remonter au conteneur parent principal
      if (target) {
        var packContainer = target.closest('.product-pack-conversion') || 
                           target.closest('product-pack') ||
                           target;
        
        // Calculer la hauteur du header
        var headerHeight = 0;
        var header = document.querySelector('.header-wrapper') || 
                     document.querySelector('.section-header') ||
                     document.querySelector('header');
        if (header) {
          headerHeight = header.offsetHeight;
        }
        
        // Position de scroll
        var rect = packContainer.getBoundingClientRect();
        var scrollTop = window.pageYOffset || document.documentElement.scrollTop;
        var targetY = rect.top + scrollTop - headerHeight - 20;
        
        // Smooth scroll
        window.scrollTo({
          top: targetY,
          behavior: 'smooth'
        });
        
        // Effet highlight violet sur le pack selector
        setTimeout(function() {
          packContainer.style.transition = 'box-shadow 0.4s ease';
          packContainer.style.boxShadow = '0 0 0 4px rgba(139, 79, 157, 0.5), 0 0 20px rgba(139, 79, 157, 0.3)';
          
          setTimeout(function() {
            packContainer.style.boxShadow = 'none';
          }, 2500);
        }, 700);
        
      } else {
        // Fallback : scroll vers le milieu bas de la page
        console.log('Pack non trouvÃ©, fallback scroll');
        var scrollTarget = document.body.scrollHeight * 0.35;
        window.scrollTo({
          top: scrollTarget,
          behavior: 'smooth'
        });
      }
    });
  })();
  
})();
</script>



